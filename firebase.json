{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "function": "nextjsFunc"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$PROJECT_DIR\" run build"
    ],
    "source": "functions"
  }
}
